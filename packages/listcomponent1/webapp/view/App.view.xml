<mvc:View controllerName="listcomponent1.controller.App"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" height="100%"
	xmlns:m="sap.m"
	xmlns="sap.f"
	>
	<m:Title text="asdas"></m:Title>
	<DynamicPage id="asd">
		<title>
			<DynamicPageTitle>
				<heading>
					<m:FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<m:Title text="{i18n>title}" wrapping="true" class="sapUiTinyMarginEnd"/>
					</m:FlexBox>
				</heading>
			</DynamicPageTitle>
		</title>
		<content>
			<m:List items="{/FlightCollection}" selectionChange=".onSelectionChange" mode="SingleSelectMaster">
				<m:StandardListItem type="Active" title="{carrid}" iconDensityAware="false" iconInset="false"/>
			</m:List>
		</content>
	</DynamicPage>
</mvc:View>
